# API URL - Firebase Functions endpoint
VITE_API_URL=https://us-central1-your-project.cloudfunctions.net/api

# Firebase Configuration (for client-side auth)
# Get these from: Firebase Console → Project Settings → Your apps → Web app
VITE_FIREBASE_API_KEY=YOUR_FIREBASE_API_KEY
VITE_FIREBASE_AUTH_DOMAIN=YOUR_AUTH_DOMAIN
VITE_FIREBASE_PROJECT_ID=YOUR_PROJECT_ID
VITE_FIREBASE_STORAGE_BUCKET=YOUR_STORAGE_BUCKET
VITE_FIREBASE_MESSAGING_SENDER_ID=YOUR_MESSAGING_SENDER_ID
VITE_FIREBASE_APP_ID=YOUR_FIREBASE_APP_ID

# Google OAuth Client ID
VITE_GOOGLE_CLIENT_ID=YOUR_GOOGLE_OAUTH_ID

# Gemini API Key (for AI features) - OPTIONAL
# Get from: https://aistudio.google.com/apikey
GEMINI_API_KEY=YOUR_GEMINI_API_KEY
